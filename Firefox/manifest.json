			"js": ["block_spam_google.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.duckduckgo.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_ddg.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.bing.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_bing.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.startpage.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_startpage.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.search.yahoo.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_yahoo.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.yandex.ru/*"],
			"run_at": "document_start",
			"js": ["block_spam_yandex.js"],
			"css": ["sesb.css"]
		}
	],

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [
		"storage",
		"unlimitedStorage",
		"webRequest"
	],

	"web_accessible_resources": ["worker.js"],

	"browser_action": {
		"default_icon": {
			"16": "icons/sesb_icon_16.png",
			"19": "icons/sesb_icon_19.png",
			"32": "icons/sesb_icon_32.png"
		},
		"default_title": "Search Engine Spam Blocker",
		"default_popup": "popup/popup.html"
	},

	"options_ui": {
		"page": "options/options.html"
	}
}