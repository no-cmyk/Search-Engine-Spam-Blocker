{
	"manifest_version": 2,
	"name": "Search Engine Spam Blocker",
	"version": "1.4.6.5",
	"description": "Blocks spam from search engines' results",
	"homepage_url": "https://github.com/no-cmyk/search-engine-spam-blocker",

	"icons": {
		"16": "icons/sesb_icon_16.png",
		"19": "icons/sesb_icon_19.png",
		"24": "icons/sesb_icon_24.png",
		"32": "icons/sesb_icon_32.png",
		"40": "icons/sesb_icon_40.png",
		"48": "icons/sesb_icon_48.png",
		"96": "icons/sesb_icon_96.png"
	},

	"content_scripts": [
		{
