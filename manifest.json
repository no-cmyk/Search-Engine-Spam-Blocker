			"js": ["background/constants.js", "content-scripts/duckduckgo.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://www.bing.com/search?*","*://www.bing.com/*/search?*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/bing.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://www.startpage.com/do/*","*://www.startpage.com/sp/*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/startpage.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://search.yahoo.com/search*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/yahoo.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://yandex.com/search/*","*://yandex.ru/search/*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/yandex.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://www.qwant.com/*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/qwant.js"],
			"css": ["background/sesb.css"]
		},
		{
			"matches": ["*://search.brave.com/*"],
			"run_at": "document_start",
			"js": ["background/constants.js", "content-scripts/brave.js"],
			"css": ["background/sesb.css"]
		}
	],
	"background": {
		"persistent": true,
		"scripts": ["background/constants.js", "background/background.js"]
	},
	"permissions": [
		"storage",
		"unlimitedStorage",
		"<all_urls>",
		"cookies",
		"tabs"
	],
	"web_accessible_resources": ["background/worker.js"],
	"browser_action": {
		"default_popup": "popup/popup.html"
	},
	"options_ui": {
		"page": "options/options.html"
	}
}