			"run_at": "document_start",
			"js": ["block_spam_google.js", "constants.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.duckduckgo.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_ddg.js", "constants.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://www.bing.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_bing.js", "constants.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://www.startpage.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_startpage.js", "constants.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://search.yahoo.com/*"],
			"run_at": "document_start",
			"js": ["block_spam_yahoo.js", "constants.js"],
			"css": ["sesb.css"]
		},
		{
			"matches": ["*://*.yandex.com/search/*","*://*.yandex.ru/search/*","*://*.yandex.com/images/*","*://*.yandex.ru/images/*"],
			"run_at": "document_start",
			"js": ["block_spam_yandex.js", "constants.js"],
			"css": ["sesb.css"]
		}
	],

	"background": {
		"persistent": true,
		"scripts": ["constants.js", "background.js"]
	},

	"permissions": [
		"storage",
		"unlimitedStorage"
	],

	"web_accessible_resources": ["worker.js"],

	"browser_action": {
		"default_icon": {
			"16": "icons/sesb_icon_16.png",
			"19": "icons/sesb_icon_19.png",
			"32": "icons/sesb_icon_32.png"
		},
		"default_title": "Search Engine Spam Blocker",
		"default_popup": "popup/popup.html"
	},

	"options_ui": {
		"page": "options/options.html"
	}
}